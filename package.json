{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run genlicenseinfo && npm run lintfix && nuxt prepare && nuxt build",
    "dev": "nuxt dev",
    "generate": "npm run genlicenseinfo && npm run lintfix && nuxt prepare && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \"js,.ts,.vue\" --ignore-path .gitignore .",
    "lintfix": "prettier --write --list-different . && npm run lint -- --fix",
    "genlicenseinfo": "license-checker --json --customPath license-format.json --out public/license-info.json"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@prettier/plugin-pug": "^3.0.0",
    "@tsconfig/strictest": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vue/language-plugin-pug": "^1.8.25",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vue-pug": "^0.6.1",
    "nuxt": "^3.8.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "pug": "^3.0.2",
    "tailwindcss": "^3.3.6",
    "vue": "^3.3.10",
    "vue-eslint-parser": "^9.3.2",
    "vue-eslint-parser-template-tokenizer-pug": "^0.4.10",
    "vue-router": "^4.2.5"
  }
}
